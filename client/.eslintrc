{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion":6,
    "sourceType": "module"
  },
  "env":{
    "browser": true,
    "es6": true,
    "node": true
  },
//  "extends": "airbnb",
    "extends": "standard",
  "rules": {
    "semi": [0],
    "linebreak-style":["warn", "windows"],
    "react/jsx-filename-extension":[0],
      "indent": [0],
      "import/extensions":"warn",
      "react/jsx-indent":[0],
      "no-unused-vars":"warn",
      "no-multiple-empty-lines":[0]
  }
}
